{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "vite",
        "path": "/boranga/frontend/boranga",
        "options": {
          "env": {
            "HOST": "127.0.0.1",
            "PORT": "5173"
          }
        },
        "type": "npm",
        "script": "dev",
        "isBackground": true,
        "problemMatcher": [
          {
            "pattern": {
              "regexp": "^([^\\s].*)[\\(:](\\d+)[,:](\\d+)(?:\\):\\s+|\\s+-\\s+)(error|warning|info)\\s+TS(\\d+)\\s*:\\s*(.*)$",
              "file": 1,
              "line": 2,
              "column": 3,
              "severity": 4,
              "code": 5,
              "message": 6
            },
            "background": {
              "activeOnStart": true,
              "beginsPattern": ".",
              "endsPattern": ".",
            }
          }
        ]
      }
    ]
  }
